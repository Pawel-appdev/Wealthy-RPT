<Window x:Class="Wealthy_RPT.Year"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wealthy_RPT"
        mc:Ignorable="d"
        Title="Report Year" Height="250" Width="400">
    <Grid>
        <Label Content="Select Report Year" HorizontalAlignment="Left" Height="28" Margin="24,31,0,0" VerticalAlignment="Top" Width="218"/>
        <ComboBox x:Name="cboReportYear" HorizontalAlignment="Left" Height="25" Margin="24,72,0,0" VerticalAlignment="Top" Width="282" DropDownClosed="cboReportYear_DropDownClosed"/>
        <Button x:Name="btnSubmit" Content="OK" HorizontalAlignment="Left" Height="28" Margin="59,132,0,0" VerticalAlignment="Top" Width="100" Click="btnSubmit_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Height="28" Margin="181,132,0,0" VerticalAlignment="Top" Width="100" Click="btnCancel_Click"/>

    </Grid>
</Window>
